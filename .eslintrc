{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "indent": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "space-before-blocks": "off",
        "no-trailing-spaces": "off",
        "max-len": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}